<struts>
    <constant name="struts.action.extension" value="do" />
    <constant name="struts.action.excludePattern" value="/api/.*" />
    <constant name="struts.devMode" value="false" />
    <constant name="struts.i18n.reload" value="true" />
    <constant name="struts.multipart.maxSize" value="26214400" />
    <constant name="struts.custom.i18n.resources" value="error_messages,application_constants,application_field_names,application_long_names,helpText" />
    <constant name="struts.ui.theme" value="simple" />
    <constant name="struts.ognl.allowStaticMethodAccess" value="true"/>
    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.mapper.action.prefix.enabled" value="true" />
    <constant name="struts.mapper.action.prefix.crossNamespaces" value="true" />
    <include file="corporate-crm.xml"/>
    <include file="whatsapp.xml"/>
    <package name="cachefilter" extends="json-default, tiles-default">
        <interceptors>
            <interceptor name="cacheIntercept" class="qa.com.qic.utility.interceptors.CacheControlInterceptor" />
            <interceptor-stack name="cacheControlStack">
                <interceptor-ref name="cacheIntercept" />
                <interceptor-ref name="defaultStack" >
                    <param name="exception.logEnabled">true</param>
                    <param name="exception.logLevel">ERROR</param>
                </interceptor-ref>
                <interceptor-ref name="token">
                    <param name="excludeMethods">*</param>
                </interceptor-ref>
            </interceptor-stack>
            <!--interceptor name="sessionIntercept" class="qa.com.qic.utility.interceptors.SessionInterceptor" />
            <interceptor-stack name="sessionStack">
                <interceptor-ref name="cacheIntercept" />
                <interceptor-ref name="sessionIntercept" />
                <interceptor-ref name="defaultStack" >
                    <param name="exception.logEnabled">true</param>
                    <param name="exception.logLevel">ERROR</param>
                </interceptor-ref>
            </interceptor-stack-->
        </interceptors>
        <default-interceptor-ref name="cacheControlStack"></default-interceptor-ref>
        <global-results>
            <result name="unhandledException">/errormsg.jsp</result>
            <result name="commonError" type="tiles">CommonErrorPage</result>
            <result name="unAuthroized">/WEB-INF/pages/uam/unAuthroizedAccess.jsp</result>
            <result name="accepted_request" type="httpheader">
                <param name="status">202</param>
                <param name="error">202</param>
                <param name="errorMessage">Transaction request accepted and processing</param>
            </result>
            <result name="ok_request" type="httpheader">
                <param name="status">200</param>
                <param name="error">200</param>
                <param name="errorMessage">Transaction request successful</param>
            </result>
            <result name="bad_request" type="httpheader">
                <param name="status">400</param>
                <param name="error">400</param>
                <param name="errorMessage">The request could not be understood by the server</param>
            </result>
            <result name="forbidden_request" type="httpheader">
                <param name="status">403</param>
                <param name="error">403</param>
                <param name="headers.Cache-Control">no-store, no-cache, must-revalidate</param>
                <param name="headers.Pragma">no-cache</param>
                <param name="headers.Expires">-1</param>
                <param name="headers.X-Frame-Options">SAMEORIGIN</param>
                <param name="errorMessage">You do not have permission to access this resource</param>
            </result>
            <result name="data" type="json">
                <param name="includeProperties">aaData.*,messageType,message,dataBean.*,recordsTotal,recordsFiltered,draw</param>
                <param name="excludeNullProperties">false</param>
                <param name="noCache">true</param>
            </result>
            <result name="failure" type="stream">
                <param name="contentType">text/html</param>
                <param name="inputName">inputStream</param>
            </result>
            <result name="session-expired" type="redirectAction">
                <param name="actionName">login</param>
                <param name="namespace">/</param>
                <param name="message">Session has expired</param>
                <param name="statusCode">307</param>
            </result>
            <result name="lock_screen" type="tiles">LockPage</result>
            <result name="logout" type="redirectAction">
                <param name="actionName">logout</param>
                <param name="namespace">/</param>
            </result>
        </global-results>
        <!--global-exception-mappings>
            <exception-mapping exception="java.lang.Exception" result="unhandledException"/>
        </global-exception-mappings-->
    </package>
    <package name="sessionfilter" extends="cachefilter">
        <interceptors>
            <interceptor name="sessionIntercept" class="qa.com.qic.utility.interceptors.SessionInterceptor" />
            <interceptor-stack name="sessionStack">
                <interceptor-ref name="cacheIntercept" />
                <interceptor-ref name="sessionIntercept" />
                <interceptor-ref name="defaultStack" >
                    <param name="exception.logEnabled">true</param>
                    <param name="exception.logLevel">ERROR</param>
                </interceptor-ref>
            </interceptor-stack>
        </interceptors>
        <default-interceptor-ref name="sessionStack"></default-interceptor-ref>
    </package>
