<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
"http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="GICustomer360" namespace="/" extends="sessionfilter"> 
         <action name="AnoudCorpIntegration" class="qa.com.qic.anoud.corporate.action.AnoudGIAction" method="anoudCorpIntegration">
                  <result name="redirect" type="redirect">${flex1}</result>
         </action> 
         <action name="GIHome" class="qa.com.qic.anoud.corporate.action.AnoudGIAction" method="loadHomePage">
            <result name="corporate" type="tiles">CorporateHome</result>
            <result name="corpPlugin" type="tiles">CorpHomePlugin</result>
            <result name="corp_data" type="json">
                <param name="includeProperties">aaData.*,bbData.*,ccData.*,messageType,polBean.*</param>
                <param name="excludeNullProperties">false</param>
                <param name="noCache">true</param>
            </result>     
        </action>           
         <action name="*GIPolicy*" class="qa.com.qic.anoud.corporate.action.AnoudGIAction" method="{1}GIPolicy{2}">
           <result name="corp_policy_page">/WEB-INF/pages/corporate/corpPolicyDetailsPage.jsp</result>
            <result name="corp_data" type="json">
                <param name="includeProperties">aaData.*,bbData.*,ccData.*,messageType,polBean.*</param>
                <param name="excludeNullProperties">false</param>
                <param name="noCache">true</param>
            </result> 
           <allowed-methods>loadGIPolicyData,loadGIPolicyChart,loadGIPolicyContacts,openGIPolicyDetailsPage,loadGIPolicyDetailsList</allowed-methods>            
        </action>  
        <action name="*GIClaim*" class="qa.com.qic.anoud.corporate.action.AnoudGIAction" method="{1}GIClaim{2}">
             <result name="corp_claim_page">/WEB-INF/pages/corporate/corpClaimDetailsPage.jsp</result>
            <result name="corp_data" type="json">
                <param name="includeProperties">aaData.*,bbData.*,ccData.*,messageType,polBean.*</param>
                <param name="excludeNullProperties">false</param>
                <param name="noCache">true</param>
            </result> 
           <allowed-methods>openGIClaimDetailsPage,loadGIClaimDetailsList</allowed-methods>            
        </action>        
        <action name="*GIActivity*" class="qa.com.qic.anoud.corporate.action.AnoudGIAction" method="{1}{2}">
            <result name="contact_form">/WEB-INF/pages/corporate/contactForm.jsp</result> 
            <result name="corp_email_form">/WEB-INF/pages/corporate/corpEmailForm.jsp</result>            
            <allowed-methods>openContactForm,saveContactForm,openEmailForm,sendEmailForm</allowed-methods>
        </action>
   </package>
   
    <package name="KPI" namespace="/" extends="sessionfilter">           
        <action name="openTargetDefinitionsPage" class="qa.com.qic.anoud.corporate.action.KPIAction" method="openTargetDefinitionsPage">
            <result name="target_definitions" type="tiles">targetDefPage</result>
        </action>
        <action name="openSalesPlanningPage" class="qa.com.qic.anoud.corporate.action.KPIAction" method="openSalesPlanningPage">
            <result name="sales_planning" type="tiles">salesPlanPage</result>
        </action>
        <action name="*Target*" class="qa.com.qic.anoud.corporate.action.KPIAction" method="{1}Target{2}">
             <result name="add_trgt_frm">/WEB-INF/pages/kpi/addTarget.jsp</result>                    
             <result name="edit_trgt_frm">/WEB-INF/pages/kpi/editTarget.jsp</result>             
             <result name="del_trgt_frm">/WEB-INF/pages/kpi/deleteTarget.jsp</result>             
             <result name="data" type="json">
                <param name="includeProperties">aaData.*,messageType,operation,approveDisabled,targetBean.*</param>
                <param name="excludeNullProperties">false</param>
                <param name="noCache">true</param>
             </result>                                
             <allowed-methods>openTargetAddForm,openTargetAmendForm,openTargetEditForm,openTargetDeleteForm,saveTargetForm,saveTargetApproval,saveTargetAmend,
                  loadAllTargets,loadApprovedTargets,loadTargetIsApproveDisabled,loadTargetMaxRevision,loadTargetAmendHistory,loadTargetTypes,
                  loadTargetLevels,loadTargetSalesPlanBusUnits,loadAllTargetLvlsForYrCompRev,loadNextTargetLevelForSalesPlan,loadCurrentTargetLevelForSalesPlan
             </allowed-methods>
         </action>                  
    </package>
</struts>